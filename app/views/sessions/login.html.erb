<h1>Log in here</h1>
<% if flash[:message] %>
  <%= flash[:message] %>
<% end %>

<%= form_for(:user) do |f| %>
<br>
<%= f.label :username %>
<%= f.text_field :username %>
<br>
<%= f.label :password %>
<%= f.password_field :password%>

<%= f.submit "Log in" %>
<% end  %>
<br>
<%= link_to "Login via GitHub", '/auth/github' %>
